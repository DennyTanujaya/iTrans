<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.pdes.itranstest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_logo_bus"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_bus_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="UnusedAttribute">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IndexActivity" android:label="@string/app_name" />
        <activity android:name=".BusActivity" android:label="@string/app_name" />
        <activity android:name=".PickingCalendarActivity" android:label="@string/app_name" />
        <activity android:name=".HasilBooking" android:label="@string/app_name" />
        <activity android:name=".MainActivity" android:label="iTrans"/>
    </application>

</manifest>